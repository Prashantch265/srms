    "nodemailer": "^6.7.5",
    "nodemailer-express-handlebars": "^5.0.0",
    "passport": "^0.6.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.0",
    "pg": "^8.7.1",
