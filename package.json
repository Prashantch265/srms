{
  "name": "srms",
  "version": "1.0.0",
  "description": "This is a summer project created as a solution for student report management.",
  "main": "server.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint --fix . && echo 'Lint complete.'",
    "start": "npm run build && cross-env NODE_ENV=production node dist/server.js",
    "dev": "cross-env NODE_ENV=development nodemon --inspect",
    "migration:run": "cross-env NODE_ENV=development npx sequelize-cli db:migrate",
    "migration:revert": "npx sequelize-cli db:migrate:undo",
    "seed:run": "npx sequelize-cli db:seed",
    "seed:revert": "npx sequelize-cli db:seed:undo"
  },
  "repository": {
    "type": "git",
    "url": "git+https://gitlab.com/Prashantch265/node-framework.git"
  },
  "keywords": [
    "Node.js",
    "Express",
    "Postgres"
  ],
  "author": "prashant",
  "license": "ISC",
  "bugs": {
    "url": "https://gitlab.com/Prashantch265/node-framework/issues"
  },
  "homepage": "https://gitlab.com/Prashantch265/node-framework#readme",
  "dependencies": {
    "bcrypt": "^5.0.1",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^10.0.0",
    "ejs": "^3.1.8",
    "express": "^4.17.1",
    "express-http-context": "^1.2.4",
    "express-session": "^1.17.2",
